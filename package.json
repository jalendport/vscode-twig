{
  "name": "vscode-twig",
  "displayName": "Twig",
  "description": "Adds Twig language support to VSCode",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.32.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "twig",
        "aliases": [
          "Twig",
          "twig"
        ],
        "extensions": [
          ".twig",
          ".html.twig"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "twig",
        "scopeName": "source.twig",
        "path": "./syntaxes/twig.tmLanguage.json",
        "embeddedLanguages": {
          "text.html": "html",
          "source.css": "css",
          "source.js": "javascript"
        }
      }
    ]
  }
}
